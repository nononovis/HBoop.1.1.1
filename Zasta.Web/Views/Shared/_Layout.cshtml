    @if (IsSectionDefined("map"))
     {
        @Scripts.Render("~/bundles/Ux/Detailmap")
       }
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
  
    @RenderSection("scripts", false)
    @if (!IsSectionDefined("map") && !IsSectionDefined("param"))
       { 
           @Scripts.Render("~/bundles/Ux/map")
         }
     @if (IsSectionDefined("monParam"))
       { 
           @Scripts.Render("~/bundles/parametre")
           @RenderSection("monParam", false)
         }
    @if (!IsSectionDefined("param"))
       { 
           @Scripts.Render("~/bundles/Init")
         }
    @Scripts.Render("~/bundles/Ux")
    @if (!IsSectionDefined("param"))
       { 
           @Scripts.Render("~/bundles/Info")
         }
</body>
    }
    else
    {
      <body>
    <noscript>
        <p class="chromeframe1">JavaScript is disabled. If you have the ability to activate, do, you will gain comfort of use.</p>
    </noscript> 
    <div id="preloader">
      <div id="status">&nbsp;</div>
    </div>
