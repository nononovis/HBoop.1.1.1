        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/Admin")
      @if (IsSectionDefined("conf"))
       { 
           @Scripts.Render("~/bundles/AdminConfirme")
           @RenderSection("conf", false)
         }
