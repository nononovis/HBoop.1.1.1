                            <ul class="dropdown-menu">
                                <li>
                                    <a href="#" title=""><img src="../../Images/fr.png" alt="" />&nbsp; FR</a>
                                </li>
                                <li>
                                    <a href="#" class="en" title=""><img src="../../Images/en.png" alt="" />&nbsp; EN</a>
                                </li>
                            </ul>
                        </li>
                    </ul>

                </div>
            </div>
        </div>
                     
            @if (!IsSectionDefined("menu"))
               {
                     <div class="header-map">
                       
                         @Html.Partial("_Menu", null)

                        <div class="sky-tabs sky-tabs-pos-left sky-tabs-anim-flip sky-tabs-response-to-icons">

                                <div class="sky-tab-content-4">
            	                    <label for="sky-tab4" class="ma5-page"> <a class="ma5-toggle-menu" title=""><i class="fa fa-navicon"></i></a></label>
                                </div>
                            <div class="info-salon"></div>
                            @*@Html.Partial("informationsalon", null)*@
                         </div>
                        <div id="map"></div>
                        
                        @Html.Partial("Recherche", null)

                        <br />
                        <h1>Trouvez un salon en quelques cliques !</h1>
                     </div>
                   }
            else
               {
                  @RenderSection("menu", false)
                  if (IsSectionDefined("param"))
                   {
                     
                      @RenderSection("param", false)
                     }
                   }
             @if (IsSectionDefined("map"))
               { 
                  @RenderSection("map", false)
                   }
            <div class="container mobile"></div>
            <div class="container body-content">
            @RenderBody()
            </div>
    
