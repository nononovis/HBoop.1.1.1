    @Styles.Render("~/Content/css")
    @if (!IsSectionDefined("NavBarClient") && !IsSectionDefined("admin"))
       {
         @Styles.Render("~/Content/style")
      }
    else if (IsSectionDefined("admin"))
       {
          @Styles.Render("~/Content/styleAdmin")
      }
    else{
    @Styles.Render("~/Content/styleClient")
    }
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,300' rel='stylesheet' type='text/css' />
</head>
@if (!IsSectionDefined("admin"))
