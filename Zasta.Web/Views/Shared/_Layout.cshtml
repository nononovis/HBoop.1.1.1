        @Html.Partial("_NavBarAdmin", null)
        <div id="cl-wrapper" class="fixed-menu sb-collapsed">
        @Html.Partial("_MenuAdmin", null)
        @RenderBody()
         </div>
        @Html.Partial("_footerAdmin", null)
