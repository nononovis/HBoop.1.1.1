        <div class="footer-below">
            <div class="container">
                 @if (!IsSectionDefined("footer"))
                 { 
                @Html.Partial("_footer", null)
                 }
                 else
                 {
                 @Html.Partial("_footerClient", null)
                     @RenderSection("footer", false)
                 }
                <div class="col-md-4 text-center">
                    <a href="http://www.smiriengineering.com/" title="smiri engineering" target="_blank" data-ajax="false">Powered by <img src="../../Images/logoseb.png" alt="Smiri Engineering" width="50"></a>
                </div>


                <div class="col-md-4 end">
                    Copyright &copy; Salon de coiffure
                </div>

            </div>
